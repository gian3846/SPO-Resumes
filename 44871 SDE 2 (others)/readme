\begin{tcolorbox}[colback=gray!30]\textcolor{black}{\textbf{CHAMPHUNT INC} $|$ \textit{Machine Learning Internship} \hfill (\textit{Sep'24--Jan'25})}
\end{tcolorbox}
\vspace{-4mm}
\begin{itemize}
    
    

    \item Built \textbf{Hybrid Post Recommender} (content + collaborative filtering), boosting relevance by \textbf{28\%} \& engagement by \textbf{3 min}

    \item Built \textbf{Q-Learning friend recommender} using Q-table updates from user interactions, achieving \textbf{78\%} user acceptance rate
    \item Integrated \textbf{location match, follower overlap}, \& \textbf{noise factors} with weight tuning, boosting \textbf{personalization} for \textbf{5k} users
  
\item Built \textbf{Cricket Ball Detection Algorithm} using \textbf{Contour Detection + YOLOv8}, \textbf{HSV masking \&  temporal checks}

    \item Applied \textbf{Gaussian blur} \& \textbf{morphological operation} to reduce noise; Used \textbf{Optical Flow} \& \textbf{Kalman Filter} for smoothing

      \item Automated extraction of \textbf{ball–pitch contact frames; classifying deliveries} as yorker, bouncer, etc. to improve analysis 


    
  

%\item Validated on \textbf{5,000+ video frames}, achieving  \textbf{$\sim$81\%} accuracy
\end{itemize}
